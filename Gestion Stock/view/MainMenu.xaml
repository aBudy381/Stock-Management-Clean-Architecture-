<Window x:Class="Gestion_Stock.View.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gestion_Stock.View"
        mc:Ignorable="d"
        Title="MainMenu" Height="450" Width="800">
    <Grid>
        <Button Margin="322,157,321.6,223" Width="150" Height="40" x:Name="Viewbutton" IsDefault="True" Content="View Article" Command="{Binding CmdView}" Click="viewbutton_Click"/>
        <Button Margin="322,242,321.6,138" Width="150" Height="40" x:Name="Managebutton" IsDefault="True" Content="Manage Article" Command="{Binding CmdManage}" Click="managebutton_Click"/>
    </Grid>
</Window>
